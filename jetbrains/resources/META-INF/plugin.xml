<idea-plugin>
  <id>com.github.sainnhe.forestnight.jetbrains</id>
  <name>Forest Night</name>
  <version>1.0.0</version>
  <vendor email="jeffreylec@gmail.com" url="https://github.com/sainnhe/forest-night">Forest Night</vendor>

  <description><![CDATA[
      Green based but warm-toned.</br>
      Designed to have soft contrast for eye protection.</br>
      Works well with redshift and f.lux.</br>
      Rich support for common file types and plugins.</br>
      </br>
      Still a work-in-progress, but is fully featured. Feel free to contribute at <a href="https://github.com/sainnhe/forest-night">forest-night</a>
    ]]></description>

  <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <depends>com.intellij.modules.platform</depends>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="9e12f552-7744-44ee-8881-891789633d84" path="/META-INF/Forest_Night.theme.json"/>
  </extensions>

  <actions>
  </actions>

</idea-plugin>
